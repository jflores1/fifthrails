<table class="table table-hover">
  <thead>
  <tr>
    <th>Item Name</th>
    <th>Storage Rate</th>
    <th>Item Status</th>
  </tr>
  </thead>
  <tbody>
      <% current_user.items.stored.each do |item| %>
          <tr>
            <td><%= item.name %></td>
            <td>$<%= item.price %></td>
            <td><%= item.item_status %></td>
          </tr>
      <% end %>
  </tbody>
</table>
